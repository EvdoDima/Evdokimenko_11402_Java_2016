<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN"
        "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module rename-to="hellogwt">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>
    <inherits name='com.google.gwt.user.theme.standard.Standard'/>


    <!-- Specify the app entry point class.                   -->
    <entry-point class='ru.kpfu.itis.gwt.client.MySampleApplication'/>

    <servlet path="/quote.rpc"
             class="ru.kpfu.itis.gwt.server.MySampleApplicationServiceImpl"/>


    <!-- Specify the app servlets.                   -->
    <!--<servlet path='/MySampleApplicationService' class='ru.kpfu.itis.gwt.server.MySampleApplicationServiceImpl'/>-->


</module>